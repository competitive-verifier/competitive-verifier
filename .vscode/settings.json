{
    "files.associations": {
        "**/*.go": "text",
        "**/_includes/**/*.html": "jekyll",
        "**/_layouts/**/*.html": "jekyll"
    },
    "emmet.includeLanguages": {
        "jekyll": "html"
    },
    "files.exclude": {
        "**/integration_test_data/dst_dir/**/cache/online-judge-tools": true,
        ".venv": true,
        "**/__pycache__": true,
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true
    },
    "python.experiments.optInto": [
        "pythonTestAdapter"
    ],
    "python.testing.pytestArgs": ["-vv"],
    "python.testing.unittestEnabled": false,
    "python.testing.pytestEnabled": true
}