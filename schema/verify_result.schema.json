{
  "$defs": {
    "FileResult": {
      "properties": {
        "verifications": {
          "description": "The results of each verification.",
          "items": {
            "$ref": "#/$defs/VerificationResult"
          },
          "title": "Verifications",
          "type": "array"
        },
        "newest": {
          "default": true,
          "description": "Whether the verification was performed on the most recent run.",
          "title": "Newest",
          "type": "boolean"
        }
      },
      "title": "FileResult",
      "type": "object"
    },
    "JudgeStatus": {
      "enum": [
        "AC",
        "WA",
        "RE",
        "TLE",
        "MLE"
      ],
      "title": "JudgeStatus",
      "type": "string"
    },
    "ResultStatus": {
      "enum": [
        "success",
        "failure",
        "skipped"
      ],
      "title": "ResultStatus",
      "type": "string"
    },
    "TestcaseResult": {
      "properties": {
        "name": {
          "description": "The name of test case.",
          "title": "Name",
          "type": "string"
        },
        "status": {
          "$ref": "#/$defs/JudgeStatus",
          "description": "The result status of the test case."
        },
        "elapsed": {
          "description": "Number of seconds elapsed for the test case.",
          "title": "Elapsed",
          "type": "number"
        },
        "memory": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "The size of memory used in megabytes.",
          "title": "Memory"
        }
      },
      "required": [
        "name",
        "status",
        "elapsed"
      ],
      "title": "TestcaseResult",
      "type": "object"
    },
    "VerificationResult": {
      "properties": {
        "verification_name": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "The name of verification.",
          "title": "Verification Name"
        },
        "status": {
          "$ref": "#/$defs/ResultStatus",
          "description": "The result status of verification."
        },
        "elapsed": {
          "description": "Total number of seconds elapsed for all test cases.",
          "title": "Elapsed",
          "type": "number"
        },
        "slowest": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Maximum number of seconds elapsed for each test cases.",
          "title": "Slowest"
        },
        "heaviest": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Maximum size of memory used in megabytes.",
          "title": "Heaviest"
        },
        "testcases": {
          "anyOf": [
            {
              "items": {
                "$ref": "#/$defs/TestcaseResult"
              },
              "type": "array"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "The results of each test case.",
          "title": "Testcases"
        },
        "last_execution_time": {
          "description": "The time at which the last validation was performed.",
          "format": "date-time",
          "title": "Last Execution Time",
          "type": "string"
        }
      },
      "required": [
        "status",
        "elapsed"
      ],
      "title": "VerificationResult",
      "type": "object"
    }
  },
  "properties": {
    "total_seconds": {
      "description": "Total number of seconds elapsed for all verification.",
      "title": "Total Seconds",
      "type": "number"
    },
    "files": {
      "additionalProperties": {
        "$ref": "#/$defs/FileResult"
      },
      "description": "The files to be verified.",
      "propertyNames": {
        "format": "path"
      },
      "title": "Files",
      "type": "object"
    }
  },
  "required": [
    "total_seconds"
  ],
  "title": "VerifyCommandResult",
  "type": "object"
}
